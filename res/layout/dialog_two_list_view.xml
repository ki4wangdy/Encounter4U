<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="@dimen/setting_user_dialog_height"
    android:gravity="center">
    
	<ListView 
	    android:id="@+id/dialog_listview_left"
	    android:layout_width="@dimen/setting_user_dialog_list_line_width"  
	    android:layout_height="wrap_content"  
	    android:cacheColorHint="@android:color/transparent"
        android:listSelector="@drawable/main_ui_listitem_bg"
        android:dividerHeight="1px"
        android:divider="@color/main_ui_line_color"
        android:fadingEdge="none"
        android:fadingEdgeLength="0dp"
        android:overScrollFooter="@null"
        android:scrollbars="none" />
	
	<ListView 
	    android:id="@+id/dialog_listview_right"
	    android:layout_width="@dimen/setting_user_dialog_list_line_width"  
	    android:layout_height="wrap_content"  
	    android:layout_toRightOf="@id/dialog_listview_left"
	    android:layout_marginLeft="@dimen/setting_user_dialog_list_margin_left"
	    android:cacheColorHint="@android:color/transparent"
        android:listSelector="@drawable/main_ui_listitem_bg"
        android:dividerHeight="1px"
        android:divider="@color/main_ui_line_color"
        android:fadingEdge="none"
        android:fadingEdgeLength="0dp"
        android:overScrollFooter="@null"
        android:scrollbars="none" />
		
</RelativeLayout>
